{
  "name": "gitlab-pr-ai-reviewer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "tsx watch server/index.ts",
    "build": "npm run vercel-build",
    "build:frontend": "tsc -b && vite build",
    "build:backend": "tsc --project tsconfig.server.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node dist/server/index.js",
    "vercel-build": "tsc -b && vite build --mode production",
    "migrate": "tsx scripts/migrate.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.0",
    "@heroicons/react": "^2.0.18",
    "@types/pg": "^8.15.4",
    "@vercel/node": "^5.3.6",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.8.1",
    "sqlite3": "^5.1.6",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/forms": "^0.5.6",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.4.24",
    "rollup": "3.29.5",
    "tsx": "^4.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "4.5.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "3.29.5",
    "@rollup/rollup-linux-x64-gnu": "3.29.5",
    "@rollup/rollup-win32-x64-msvc": "3.29.5"
  }
}